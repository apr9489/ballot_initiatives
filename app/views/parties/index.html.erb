<div class="row-fluid">
<h1>Listing parties</h1>
 <div class="span12">
      <div class="span11">
        <table class="table table-hover">
          <thead class="unstyled inline">
  <tr>
    <th>Name</th>
    <th>Website</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 </thead>
<% @parties.each do |party| %>
<tbody class="unstyled inline">
  <tr>
    <td><%= party.name %></td>
    <td><%= party.website %></td>
    <td><%= link_to 'Show', party %></td>
    <td><%= link_to 'Edit', edit_party_path(party) %></td>
    <td><%= link_to 'Destroy', party, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
 </tbody>
        </table>
      </div>
    </div>
  <span class="btn btn-primary"><%= link_to 'New Party', new_party_path %></span>
  </div>
