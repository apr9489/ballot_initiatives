<div class="row-fluid">
<h1>Listing clubs</h1>
 <div class="span12">
      <div class="span11">
        <table class="table table-hover">
          <thead class="unstyled inline">
  <tr>
    <th>Name</th>
    <th>Party</th>
    <th>Website</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
</thead>
<% @clubs.each do |club| %>
 <tbody class="unstyled inline">
  <tr>
    <td><%= club.name %></td>
    <td><%= club.party.name %></td>
    <td><%= club.website %></td>
    <td><%= link_to 'Show', club %></td>
    <td><%= link_to 'Edit', edit_club_path(club) %></td>
    <td><%= link_to 'Destroy', club, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</tbody>
        </table>
      </div>
    </div>
  <span class="btn btn-primary"><%= link_to 'New Club', new_club_path %></span>
  </div>
